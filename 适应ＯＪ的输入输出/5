#include<stdio.h>
int main()
{
    int N,M;
    int n;
    int sum;
    int i;
    scanf("%d",&N);
    for (i = 0 ; i < N ; i++)
    {
        sum = 0;
        scanf("%d", &M);
        while(M--)
        {
            scanf("%d", &n);
            sum += n;
        }
        printf("%d\n", sum);
    }
}
